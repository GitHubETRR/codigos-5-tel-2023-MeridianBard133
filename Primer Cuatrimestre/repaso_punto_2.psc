Algoritmo repaso_punto_2
	Escribir 'Por favor ingrese un ángulo'
	Leer Ang
	Escribir 'Por favor ingrese un módulo'
	Leer Modu
	real <- Modu*COS(Ang)
	imag <- Modu*SEN(Ang)
	Escribir 'El número real dio',real
	Escribir 'El número imaginario de la cuenta es',imag,'J'
	Escribir 'Por lo tanto, la forma rectangular es',real,imag,'J'
FinAlgoritmo
